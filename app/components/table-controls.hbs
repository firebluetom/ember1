<div class="table-controls">

  <Checkbox
    @partial={{this.showPartial}}
    @checked={{this.showFull}}
    @onClick={{@onClickCheckbox}}
  />

  <span class="count-selected" tabindex="0" data-test-selected-count>{{this.selectedText}}</span>

  <DownloadSelected 
    @onClick={{@onClickDownload}}
  />
</div>